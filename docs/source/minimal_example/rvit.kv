FloatLayout:
    PointRenderer:
        pos_hint: {'x':0.0, 'y':0.0}
        size_hint:  (1.0,1.0)
        x_data: 'pos[:,0]'
        y_data: 'pos[:,1]'
        point_size: 0.25
        color: [0.0,1.0,1.0,0.25]
        unique_name: 'pos'
